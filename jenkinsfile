pipeline {
agent any
stages {
stage('checkout') {
steps {
git branch :'main', url: 'https://github.com/Divz1991/jenkins-maven-demo.git'
}
}
stage('Build') {
steps {
sh 'mvn clean package'
}
}
stage ('Test') {
steps {
sh 'mvn test'
}
}
stage ('Report')
{
steps {
junit 'target/surefire-reports/*.xml'
}
}
}
}
